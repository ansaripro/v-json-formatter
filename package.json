{
  "name": "v-json-formatter",
  "private": false,
  "version": "0.0.2",
  "author": "ansaripro",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "json",
    "formatter",
    "transformation",
    "editor",
    "vue3",
    "vue",
    "vuejs"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/v-json-formatter.umd.cjs",
  "module": "./dist/v-json-formatter.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/v-json-formatter.js",
      "require": "./dist/v-json-formatter.umd.cjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./dist/index.d.ts": {
      "import": "./dist/index.d.ts",
      "require": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ansaripro/v-json-formatter.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "docs:dev": "vitepress dev documentation",
    "docs:build": "vitepress build documentation",
    "docs:serve": "vitepress serve documentation",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "vue": "^3.4.21",
    "vuedraggable": "^4.1.0"
  },
  "peerDependencies": {
    "vue": "^3.4.21",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.4.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "postcss": "^8.4.41",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vitepress": "^1.3.4",
    "vue-tsc": "^2.0.6"
  }
}
